{
    "root": true,
    "extends" :  [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
    "no-unused-vars": [
        "error",
        {
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
        }
    ],
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}